                        <ul for="naloga<%= i+2 %>" class="mdl-menu mdl-menu--bottom-left mdl-js-menu">
                            <li class="mdl-menu__item" data-val="DONE">Opravljeno</li>
                            <li class="mdl-menu__item" data-val="TODO">Neopravljeno</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd"></div>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd"></div>
    <% } %>
    <% if (naloge.length > (i+3)) { %>
        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet paginate" id="card<%= i+3 %>">
        <div class="mdl-card mdl-card-search mdl-shadow--2dp overflow-visible" style="z-index: 3;">
            <div class="mdl-card__title mdl-color--orange-300 padding-10">
                <div class="naloge-title_type">
                    <div class="mdl-card__subtitle-text mdl-color-text--black"><%= naloge[i+3].kategorija %></div>
