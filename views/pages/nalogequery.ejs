                        <span class="mdl-color-text--black"><%= naloge[i+1].ime %></span>
                        <span class="mdl-list__item-text-body mdl-color-text--grey"><%= naloge[i+1].opis %></span>
                    </span>
                </div>
                <div class="mdl-list__item mdl-list__item--two-line padding-10">
                    <span class="mdl-list__item-primary-content">
                        <span class="mdl-list__item-sub-title mdl-color-text--grey">Za:</span>
                    </span>
                    <span class="mdl-list__item-primary-content">
                        <span class="mdl-list__item-sub-title mdl-color-text--grey">Avtor:</span>
                    </span>
                </div>
                <i class="material-icons" style="padding-left: 10px;vertical-align: middle">event</i>
                <span>12/1/2018 12:00 - 18:00</span>
                <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label  getmdl-select select-naloga">
                    <input type="text" value="" class="mdl-textfield__input" id="naloga<%= i+1 %>" readonly>
                    <input type="hidden" value="<%= naloge[i+1].status %>" name="naloga">
                    <i class="mdl-icon-toggle__label material-icons">keyboard_arrow_down</i>
                    <label for="naloga<%= i+1 %>" class="mdl-textfield__label">Status naloge</label>
                    <ul for="naloga<%= i+1 %>" class="mdl-menu mdl-menu--bottom-left mdl-js-menu">
                        <li class="mdl-menu__item" data-val="DONE" >Opravljeno</li>
                        <li class="mdl-menu__item" data-val="TODO">Neopravljeno</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <% } %>
    <% if (naloge.length > (i+2)) { %>
        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet paginate" id="card<%= i+2 %>">
            <div class="mdl-card mdl-card-search mdl-shadow--2dp overflow-visible" style="z-index: 4;" >
                <div class="mdl-card__title mdl-color--orange-300 padding-10">
                    <div class="naloge-title_type">
                        <div class="mdl-card__subtitle-text mdl-color-text--black"><%= naloge[i+2].kategorija %></div>
