<% for (var i = 0; i < naloge.length; i++) { %>
    <% if (i ==  0 ) { %>
        <div class="mdl-cell mdl-cell--1-col mdl-cell--hide-tablet mdl-cell--hide-phone paginate"></div>
    <% } %>
    <div class="mdl-cell mdl-cell--3-hd mdl-cell--5-col-desktop mdl-cell--4-col-tablet paginate" id="card<%= i %>">
        <div class="mdl-card mdl-card-search mdl-shadow--2dp overflow-visible" style="z-index: 6;" >
            <div class="mdl-card__title mdl-color--orange-300 padding-10">
                <div class="naloge-title_type">
                    <div class="mdl-card__subtitle-text mdl-color-text--black"><%= naloge[i].kategorija %></div>
                    <div class="mdl-card__title-text mdl-color-text--white" style="display: block;">Skupni cilji dru≈æine
                        <span class="mdl-chip mdl-color--white mdl-color-text--black mdl-chip-naloge">
                            <span class="mdl-chip__text">+<%= naloge[i].xp %> xp</span>
                        </span>
                    </div>
                </div>
            </div>
            <div class="mdl-card__supporting-text padding-zero overflow-visible">
                <div class="mdl-list__item mdl-list__item--three-line padding-10">
                            <span class="mdl-list__item-primary-content">
                                <span class="mdl-color-text--black"><%= naloge[i].ime %></span>
                                <span class="mdl-list__item-text-body mdl-color-text--grey"><%= naloge[i].opis %></span>
                            </span>
                </div>
                <div class="mdl-list__item mdl-list__item--three-line padding-10">
                            <span class="mdl-list__item-primary-content">
                                <span class="mdl-color-text--grey">Za:</span>
                                <span class="mdl-list__item-text-body chip-no-margin" style="margin-top: 10px;">
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">A</span>
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">B</span>
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">C</span>
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">D</span>
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">E</span>
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">F</span>
                                </span>
                            </span>
                    <span class="mdl-list__item-primary-content">
                                <span class="mdl-color-text--grey">Avtor:</span>
                                <span class="mdl-list__item-text-body" style="margin-top: 10px;">
                                    <span class="mdl-chip__contact mdl-color--teal mdl-color-text--white">D</span>
                                </span>
                            </span>
                </div>
                <i class="material-icons" style="padding-left: 10px;vertical-align: middle">event</i>
                <% if (naloge[i].zacetek) { %><span class="mdl-list__item-sub-title mdl-color-text--grey">Od <%= moment(naloge[i].zacetek).format( 'DD-MMM-YYYY') %> 12:00</span><% } %>
                <% if (naloge[i].konec) { %><span class="mdl-list__item-sub-title mdl-color-text--grey">do <%= moment(naloge[i].konec).format( 'DD-MMM-YYYY') %> 18:00</span><% } %>
                <% if (!naloge[i].zacetek && !naloge[i].konec) { %><span class="mdl-list__item-sub-title mdl-color-text--grey">Datum za to nalogo ni definiran.</span><% } %>
            </div>
            <div class="mdl-card__supporting-text mdl-list__item-sub-title mdl-color-text--grey">
                Status naloge:
                <% if (naloge[i].status) { %><span name="status" class="mdl-list__item-sub-title mdl-color-text--grey">Opravljena</span>
                <% } else { %><span name="status" class="mdl-list__item-sub-title mdl-color-text--grey">Neopravljena</span><% } %>
            </div>
        </div>
    </div>
    <% if ((i+1)%4 == 0 || (i+1)%2 == 0) { %>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd" name="nal2"></div>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd" name="nal2"></div>
    <% } %>
    <% if ((i+1)%4 == 0) { %>
        <div <div class="mdl-cell mdl-cell--1-col mdl-cell-show-hd hide-element" name="nal4"></div>
        <div <div class="mdl-cell mdl-cell--1-col mdl-cell-show-hd hide-element" name="nal4"></div>
    <% } %>
<% } %>