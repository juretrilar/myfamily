<div class="text-container mdl-grid">
    <div class="mdl-cell mdl-cell--1-col mdl-cell--hide-tablet mdl-cell--hide-phone"></div>
    <div class="text-main text-content color-cyan krivulja-6 mdl-shadow--4dp content mdl-color-text--grey-800 mdl-cell mdl-cell--10-col mdl-grid">
        <div class="mdl-cell mdl-cell--5-col mdl-cell--6-col-desktop mdl-cell--8-col-tablet mdl-grid--no-spacing">
            <div class="mdl-card section__text color-transparent" style="width: 100%">
                <div class="mdl-card__title">
                    <h2 class="mdl-card__title-text mdl-color-text--white shadow-text">Opomniki</h2>
                </div>
                <ul class=" mdl-list margin-zero padding-zero" id="opomnikiList">
                    <% for(let i=0;i<opomniki.length;i++) { %>
                    <li id="opomnikiDashboard" class="mdl-list__item mdl-list__item--two-line">
                        <!--<div class="circle-container-main mdl-color--grey mdl-cell--hide-phone mdl-cell--hide-tablet"></div>-->
                        <span class="mdl-list__item-primary-content">
                            <span class="mdl-color-text--white shadow-text"><%= opomniki[i].ime %></span>
                            <span class="mdl-chip mdl-color-text--white color-red mdl-chip-small">
                                <span class="mdl-chip__text">100 xp</span>
                            </span>
                            <span class="mdl-list__item-sub-title color-dark-text"><%= opomniki[i].avtor[1] %></span>
                        </span>
                        <span class="mdl-list__item-secondary-content">
                            <input type="hidden" value="<%= opomniki[i].kategorija %>">
                            <input type="hidden" value="<%= opomniki[i].zacetek %>">
                            <input type="hidden" value="<%= opomniki[i].konec %>">
                            <input type="hidden" value="<%= opomniki[i].vezan_cilj %>">
                            <input type="hidden" value="<%= opomniki[i].status %>">
                            <div class="hide-element chip-no-margin">
                            <% for(let j=0;j<opomniki[i].vezani_uporabniki.length;j++) { %>
                                <img class="mdl-chip__contact author-chip" src="<%= opomniki[i].vezani_uporabniki[j][0] %>" title="<%= opomniki[i].vezani_uporabniki[j][1] %>">
                            <% } %>  
                            </div> 
                            <img class="mdl-chip__contact author-chip hide-element" src="<%= opomniki[i].avtor[0] %>" title="<%= opomniki[i].avtor[1] %>">
                                                     <!--
                            <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect" for="opomnik<%= i %>">
                                <input type="checkbox" id="opomnik<%= i %>" name="checkbox" class="mdl-checkbox__input" <% if (opomniki[i].status == true) { %>checked="checked" <% } %>/>
                            </label>
                            -->
                        </span>
                    </li>
                    <% } %>
                </ul>
            </div>
        <!--
        <div class="mdl-card section__text mdl-color--primary" style="width: 100%">
            <div class="mdl-card__title">
                <h2 class="mdl-card__title-text mdl-color-text--white">Fizična aktivnost moje družine</h2>
            </div>
            <div class="mdl-card__supporting-text padding-zero">
                <canvas id="myChart" style="min-height: 300px"></canvas>
            </div>
        </div>
        -->
        </div>
        <div class="mdl-card section__text mdl-cell color-transparent mdl-cell--5-col mdl-cell--6-col-desktop mdl-cell--8-col-tablet overflow-visible">
            <div class="flex-container">
                <div class="mdl-textfield mdl-js-textfield mdl-color-text--white shadow-text mdl-textfield__status">
                    <input class="mdl-textfield__input not-empty" id="currStatus" type="text" id="currStatus" name="currStatus" maxlength="120" autocomplete="false">
                    <label class="mdl-textfield__label mdl-color-text--white shadow-text" for="currStatus">Moj status</label>
            </div>
            <button class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored color-dark-cyan button-objava" onclick="objaviStatus()">
                <i class="material-icons">done</i>&nbspObjavi
            </button>
            </div>           
            <div class="container">
                <div class="row">
                <% for(let j=0; j<uporabniki.length;j++) {%>
                    <% if(uporabniki[j].vrsta == 7) { %>
                        <div class="item">
                        <img src="<%= uporabniki[j].slika %>" alt="" width="64" height="64" class="level7 user">  
                        <input type="hidden" value="<%= uporabniki[j].ime %>">
                        <input type="hidden" value="<%= uporabniki[j].telefon %>">
                        <input type="hidden" value="<%= uporabniki[j].email %>">
                        <% if(uporabniki[j].status) { %>
                            <span class="mdl-chip caption">
                                    <span class="mdl-chip__text color-dark-text" id="status<%= uporabniki[j].id %>"><%= uporabniki[j].status %></span>
                            </span>
                        <% } %>
                    </div>
                    <% } %>
                <% } %>
                </div>
                <div class="row">
                <% for(let j=0; j<uporabniki.length;j++) {%>
                    <% if(uporabniki[j].vrsta == 6) { %>
                        <div class="item">
                        <img src="<%= uporabniki[j].slika %>" alt="" width="64" height="64" class="level6 user">
                        <input type="hidden" value="<%= uporabniki[j].ime %>">
                        <input type="hidden" value="<%= uporabniki[j].telefon %>">
                        <input type="hidden" value="<%= uporabniki[j].email %>">
                        <% if(uporabniki[j].status) { %>
                            <span class="mdl-chip caption">
                                    <span class="mdl-chip__text color-dark-text" id="status<%= uporabniki[j].id %>"><%= uporabniki[j].status %></span>
                            </span>
                        <% } %>
                    </div>
                    <% } %>
                <% } %>
                </div>
                <div class="row">
                <% for(let j=0; j<uporabniki.length;j++) {%>
                    <% if(uporabniki[j].vrsta == 5) { %>
                        <div class="item">
                        <img src="<%= uporabniki[j].slika %>" alt="" width="64" height="64" class="level5 user">
                        <input type="hidden" value="<%= uporabniki[j].ime %>">
                        <input type="hidden" value="<%= uporabniki[j].telefon %>">
                        <input type="hidden" value="<%= uporabniki[j].email %>">
                        <% if(uporabniki[j].status) { %>
                            <span class="mdl-chip caption">
                                    <span class="mdl-chip__text color-dark-text" id="status<%= uporabniki[j].id %>"><%= uporabniki[j].status %></span>
                            </span>
                        <% } %>
                    </div>
                    <% } %>
                <% } %>
                </div>
                <div class="row">
                    <% for(let j=0; j<uporabniki.length;j++) {%>
                        <% if(uporabniki[j].vrsta == 4) { %>
                            <div class="item">
                            <img src="<%= uporabniki[j].slika %>" alt="" width="64" height="64" class="level4 user">
                            <input type="hidden" value="<%= uporabniki[j].ime %>">
                            <input type="hidden" value="<%= uporabniki[j].telefon %>">
                            <input type="hidden" value="<%= uporabniki[j].email %>">
                            <% if(uporabniki[j].status) { %>
                                <span class="mdl-chip caption">
                                        <span class="mdl-chip__text color-dark-text" id="status<%= uporabniki[j].id %>"><%= uporabniki[j].status %></span>
                                </span>
                            <% } %> 
                        </div>
                        <% } %>
                    <% } %>
                </div>
                <div class="row">
                    <% for(let j=0; j<uporabniki.length;j++) {%>
                        <% if(uporabniki[j].vrsta == 3) { %>
                            <div class="item">
                            <img src="<%= uporabniki[j].slika %>" alt="" width="64" height="64" class="level3 user">
                            <input type="hidden" value="<%= uporabniki[j].ime %>">
                            <input type="hidden" value="<%= uporabniki[j].telefon %>">
                            <input type="hidden" value="<%= uporabniki[j].email %>">
                            <% if(uporabniki[j].status) { %>
                                <span class="mdl-chip caption">
                                        <span class="mdl-chip__text color-dark-text" id="status<%= uporabniki[j].id %>"><%= uporabniki[j].status %></span>
                                </span>
                            <% } %>
                        </div>
                        <% } %>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="text-container mdl-grid">
    <div class="mdl-cell mdl-cell--1-col mdl-cell--hide-tablet mdl-cell--hide-phone"></div>
    <div class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--5-col mdl-cell--8-col-tablet">
        <div class="mdl-card__title color-blue krivulja-2">
            <h2 class="mdl-card__title-text mdl-color-text--white shadow-text">Skupni cilji družine</h2>
            <div class="mdl-card__actions">
                <button id="dodajcilj" class="mdl-button mdl-js-button mdl-button--raised color-dark-blue align-right mdl-color-text--white" onclick="dodajNovCilj()">
                    DODAJ NOV CILJ
                    <i class="material-icons">add</i>
                </button>
            </div>
        </div>
        <ul class=" mdl-list" id="skupniCilji">
            <% if(skupniCilji.length == 0) { %>
            <li class="mdl-list__item mdl-list__item--two-line">
                <!--<div class="circle-container-main mdl-color--grey"></div>-->
                <span class="mdl-list__item-primary-content">
                    <span class="mdl-color-text--black">Družina še nima skupnih ciljev</span>
                    <span class="mdl-list__item-sub-title mdl-color-text--grey">Med dodajanjem cilja označi polje skupni cilj za prikaz na dashboardu</span>
                </span>
            </li>
            <% } %>
            <% for(let i=0;i<skupniCilji.length;i++) { %>                
            <li class="mdl-list__item mdl-list__item--two-line">
                <!--<div class="circle-container-main mdl-color--grey"></div>-->
                <span class="mdl-list__item-primary-content">
                    <span class="mdl-color-text--black"><%= skupniCilji[i].ime %></span>
                    <span class="mdl-list__item-sub-title mdl-color-text--grey"><%= skupniCilji[i].opis %></span>
                </span>
                <span class="mdl-list__item-secondary-content">
                    <label class="mdl-checkbox mdl-js-checkbox mdl-js-ripple-effect" for="skupniCilji<%= i %>">
                        <input type="checkbox" id="skupniCilji<%= i %>" class="mdl-checkbox__input" <% if (skupniCilji[i].status == true) { %>checked="checked" <% } %>/>
                    </label>
                </span>
            </li>
            <% } %>
        </ul>
    </div>
    <div class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--5-col mdl-cell--8-col-tablet">
        <div class="mdl-card__title color-red krivulja-1">
            <h2 class="mdl-card__title-text mdl-color-text--white shadow-text">Predlog za aktivnost</h2>
            <div class="mdl-card__actions">
                <button id="dodajnalogo" class="mdl-button mdl-js-button mdl-button--raised color-dark-red align-right mdl-color-text--white" onclick="dodajPredlog()">
                    DODAJ MED NALOGE
                    <i class="material-icons">add</i>
                </button>
            </div>
        </div>
        <div class="mdl-card__supporting-text">
            <div class="mdl-list">
                <div class="mdl-list__item">
                            <span class="mdl-list__item-primary-content">
                                <span class="mdl-color-text--black mdl-typography--font-bold" id="ime_aktivnosti">Moški večer</span>
                                    <span class="mdl-chip mdl-color--orange mdl-color-text--white mdl-chip-small">
                                        <span class="mdl-chip__text">100 xp</span>
                                    </span>
                            </span>
                </div>
            </div>
            <p style="padding-left: 16px;" id="opis_aktivnosti">Druženje očetov in sinov vseh generacij. Pohod med vinogradi.</p>
            <!--<h6 class="mdl-color-text--black" id="clani_aktivnosti" style="padding-left: 16px;">Za: moški del družine (Janez, Jan, Janko)</h6>-->
        </div>
    </div>
</div>
<div id="dashboardNaloga" class="mdl-card mdl-shadow--2dp overflow-visible naloge-okno hide-element">
        <button class="mdl-button mdl-js-ripple-effect mdl-js-button mdl-button--fab color-dark closeX closeDialog" id="buttonClose">
            <i class="material-icons mdl-color-text--white" role="presentation">close</i>
            <span class="visuallyhidden">add</span>
        </button>
        <div class="mdl-card__title color-red krivulja-3 padding-10">
            <div class="naloge-title_type">
                <div class="mdl-card__subtitle-text mdl-color-text--black" id="opomnikKategorija"></div>
                <div class="mdl-card__title-text mdl-color-text--white" id="opomnikCilj" style="display: block;">
                    <span class="mdl-chip mdl-color--white mdl-color-text--black mdl-chip-naloge mdl-chip-small">
                        <span class="mdl-chip__text" id="opomnikXp"></span>
                    </span>
                </div>
            </div>
        </div>
        <div class="mdl-card__supporting-text padding-zero overflow-visible">
            <div class="mdl-list__item mdl-list__item--three-line ime-opis-naloge--container">
                <span class="mdl-list__item-primary-content">
                    <span class="mdl-color-text--black" id="opomnikIme"></span>
                    <span class="mdl-list__item-text-body mdl-color-text--grey" id="opomnikOpis"></span>
                </span>
            </div>
            <div class="mdl-list__item mdl-list__item--three-line clani-naloge--container">
                <span class="mdl-list__item-primary-content">
                    <span class="mdl-color-text--grey">Udeleženci naloge:</span>
                    <span class="mdl-list__item-text-body chip-no-margin" id="opomnikVezani">
                    </span>
                </span>
                <span class="mdl-list__item-primary-content">
                    <span class="mdl-color-text--grey">Avtor:</span>
                    <span class="mdl-list__item-text-body" id="opomnikAvtor">
                    </span>
                </span>
            </div>
            <i class="material-icons" style="padding-left: 10px;vertical-align: middle">event</i>
            <span class="mdl-list__item-sub-title mdl-color-text--grey" id="opomnikZacetek"></span>
            <span class="mdl-list__item-sub-title mdl-color-text--grey" id="opomnikKonec"></span>
            <span class="mdl-list__item-sub-title mdl-color-text--grey hide-element" id="opomnikBrezDatuma">Datum za to nalogo ni definiran.</span>
        </div>
        <div class="mdl-card__supporting-text mdl-list__item-sub-title mdl-color-text--grey">
            Status naloge:
            <span name="status" class="mdl-list__item-sub-title mdl-color-text--grey" id="status"></span>
        </div>
    </div>