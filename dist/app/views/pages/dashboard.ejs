                    <input class="mdl-textfield__input mdl-textfield__status not-empty" id="currStatus" type="text" id="currStatus" name="currStatus" maxlength="120">
                    <label class="mdl-textfield__label mdl-color-text--grey" for="sample3">Moj status</label>
                    <button class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored mdl-color--blue-800" onclick="objaviStatus()" style="position:relative;float: right; width: 20%; bottom: 10px;">
                        <i class="material-icons">done</i>&nbspObjavi
