<% for (var i = 0; i < naloge.length; i++) { %>
    <% if (i ==  0 ) { %>
        <div class="mdl-cell mdl-cell--1-col mdl-cell--hide-tablet mdl-cell--hide-phone" id="cell<%= i %>"></div>
    <% } %>
    <div class="mdl-cell mdl-cell--3-hd mdl-cell--5-col-desktop mdl-cell--4-col-tablet paginate" id="card<%= i %>">
        <div class="mdl-card mdl-card-search mdl-shadow--2dp overflow-visible" style="z-index: 6;" >
            <div class="mdl-card__title <%= shortId(naloge[i].kategorija) %> padding-10">
                <div class="naloge-title_type">
                    <div class="mdl-card__subtitle-text mdl-color-text--grey-800"><%= kategorija[naloge[i].kategorija] %></div>
                    <input type="hidden" value="<%= naloge[i].kategorija %>" class="kategorijaNaloga">
                    <input type="hidden" value="<%= naloge[i].vezan_cilj %>" class="ciljNaloga">
                    <input type="hidden" value="<%= naloge[i]._id %>" class="idNaloga">
                    <div class="mdl-card__title-text mdl-color-text--white" style="display: block;">
                        <span class="shadow-text queryIme"><%= naloge[i].ime %></span>
                        <!--<span class="shadow-text"><%= imeCilj[i]  %></span>-->
                        <span class="mdl-chip mdl-color--white mdl-color-text--black mdl-chip-naloge mdl-chip-small">
                            <span class="mdl-chip__text mdl-color-text--primary xpNaloga"><%= naloge[i].xp %> točk</span>
                        </span>
                    </div>
                </div>
            </div>
            <div class="mdl-card__supporting-text padding-zero overflow-visible">         
                <p class="mdl-card__supporting-text mdl-color-text--grey padding-10lr padding-top-16 margin-zero">Opis:</p>            
                <p class="mdl-color-text--grey-700 mdl-typography--subhead margin-zero padding-10lr queryOpis"><%= naloge[i].opis %></p>
                <p class="mdl-card__supporting-text mdl-color-text--grey padding-10lr padding-top-16 margin-zero">Cilj:</p>            
                <p class="mdl-color-text--grey-700 mdl-typography--subhead margin-zero padding-10lr"><%= imeCilj[i]  %></p>
                <div class="mdl-list__item mdl-list__item--three-line clani-naloge--container naloge-person">
                    <span class="mdl-list__item-primary-content">
                        <span class="mdl-card__supporting-text mdl-color-text--grey padding-zero">Udeleženci naloge:</span>
                        <span class="mdl-list__item-text-body chip-no-margin">
                            <% for (let j = 0; j<naloge[i].vezani_uporabniki.length;j++) { %>
                                <% if(slika[naloge[i].vezani_uporabniki[j]]) { %>
                                <img class="mdl-chip__contact author-chip" src="<%= slika[naloge[i].vezani_uporabniki[j]][0] %>" title="<%= slika[naloge[i].vezani_uporabniki[j]][2] %>">
                                <input type="hidden" value="<%= slika[naloge[i].vezani_uporabniki[j]][1] %>" class="udelezenecNaloga">
                                <% } %>
                            <% } %>
                        </span>
                    </span>
                    <span class="mdl-list__item-primary-content">
                        <span class="mdl-card__supporting-text mdl-color-text--grey padding-zero">Avtor:</span>
                        <span class="mdl-list__item-text-body">
                                <img class="mdl-chip__contact author-chip" src="<%= slika[naloge[i].avtor][0] %>" title="<%= slika[naloge[i].avtor][2] %>">
                        </span>
                    </span>
                </div>
                <ul class="'mdl-list padding-zero margin-zero">
                    <li class="mdl-list__item padding-10">
                        <span class="mdl-list__item-primary-content">                          
                            <% if (naloge[i].zacetek) { %><img class="mdl-icon-toggle__label" src="public/images/event_launch_24px.svg" title="Začetek"><span class="mdl-color-text--grey-700 mdl-typography--subhead padding-10 dateNaloga"><%= moment(naloge[i].zacetek).format('DD. MM. YYYY') +" "+ moment(naloge[i].zacetek).format('HH:mm')  %></span><% } %>
                            <% if (naloge[i].konec) { %><img class="mdl-icon-toggle__label" src="public/images/event_finish_24px.svg" title="Konec"><span class="mdl-color-text--grey-700 mdl-typography--subhead padding-10 dateNaloga"><%= moment(naloge[i].konec).format('DD. MM. YYYY') +" "+ moment(naloge[i].konec).format('HH:mm')  %></span><% } %>
                            <% if (!naloge[i].zacetek && !naloge[i].konec) { %><span class="mdl-color-text--grey-700 mdl-typography--subhead padding-10">Datum za to nalogo ni definiran.</span><% } %>
                        </span>
                    </li>
                </ul>
            </div>
            <div class="mdl-card__supporting-text mdl-color-text--grey padding-10">
                Status naloge:
                <% if (naloge[i].status) { %><span class="mdl-color-text--grey-700 mdl-typography--subhead statusNaloga">Opravljena</span>
                <% } else { %><span class="mdl-color-text--grey-700 mdl-typography--subhead statusNaloga">Neopravljena</span><% } %>
            </div>
            <button class="mdl-button mdl-js-ripple-effect mdl-js-button mdl-button--fab editmode">
                <i class="material-icons mdl-color-text--black" role="presentation">edit</i>
                <span class="visuallyhidden">add</span>
            </button>
        </div>
    </div>
    <% if ((i+1)%4 == 0 || (i+1)%2 == 0) { %>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd naloge-center" id="cellPa<%= i %>"></div>
        <div class="mdl-cell mdl-cell--1-col mdl-hide-small mdl-cell--hide-hd naloge-center" id="cellPb<%= i %>"></div>
    <% } %>
    <% if ((i+1)%4 == 0) { %>
        <div class="mdl-cell mdl-cell--1-col mdl-cell-show-hd hide-element naloge-center" id="cellHa<%= i %>"></div>
        <div class="mdl-cell mdl-cell--1-col mdl-cell-show-hd hide-element naloge-center" id="cellHb<%= i %>"></div>
    <% } %>
<% } %>